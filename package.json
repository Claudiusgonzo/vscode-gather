{
  "name": "gather",
  "displayName": "Gather",
  "description": "Adds gather functionality to the Python Extension",
  "version": "0.0.1",
  "publisher": "ms-python",
  "homepage": "https://github.com/Microsoft/vscode-python",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode-gather"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/vscode-python/issues"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:gather-for-the-python-extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gather-for-the-python-extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.46.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@msrvida/python-program-analysis": "^0.4.31",
    "@jupyterlab/coreutils": "^3.1.0",
    "@types/uuid": "^3.4.3",
    "uuid": "^3.3.2"
  }
}
